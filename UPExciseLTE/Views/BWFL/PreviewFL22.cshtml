@model UPExciseLTE.Models.FormFL21

@{
    ViewBag.Title = "PreviewFL22";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
        function PrintPanel() {
            var panel = document.getElementById("divPrint");
            var printWindow = window.open('', '', 'height=400,width=800');
            printWindow.document.write('<html><head><title>FORM F.L.-22</title>');
            printWindow.document.write('</head><body >');
            printWindow.document.write(panel.innerHTML);
            printWindow.document.write('</body></html>');
            printWindow.document.close();
            setTimeout(function () {
                printWindow.print();
            }, 500);
            return false;
        }
</script>
<div style="text-align:right;width:100%;background-color:white;">
    <button id="btnPrint" onclick="PrintPanel();"  class="btn green" style="margin-right:50px;margin-top:50px">Print FL 22</button>
</div>
<div style="width:100%;background-color:white" id="divPrint">
    <center>
        <table style="width:85%;background-color:white;text-orientation:upright">
            <tr>
                <td colspan="4" style="text-align:center"> <h2><strong> FORM F.L.-22</strong></h2></td>
            </tr>
            <tr>
                <td colspan="4" style="text-align:center"> <h2>Lucknow District, Uttar Pradesh</h2></td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="3">Permit for the import of Beer No</td>
                <td>Dated</td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>Current to</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td style="width:4%">1.</td>
                <td>Name and Address of consigner</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.FromConsignorName)</td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.FromConsignorAddress)</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>2.</td>
                <td>Name and Address of consignee</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.ToConsigeeName)</td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.ToConsigeeAddress)</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>3.</td>
                <td>Description of Beer</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.Brand) (@Html.DisplayFor(model => model.QuantityInBottleML) ml.)</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>4.</td>
                <td>Quantity of Each Kind of Beer</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.Brand) (@Html.DisplayFor(model => model.TotalCase)  C/s (@Html.DisplayFor(model => model.TotalBottle) @Html.DisplayFor(model => model.PackagingType))</td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.TotalBL) B.L.</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>5.</td>
                <td>Route</td>
                <td>&nbsp;</td>
                <td>@Html.DisplayFor(model => model.RouteDetails)</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>6.</td>
                <td>Rate of Duty per Bottle/Litre</td>
                <td>&nbsp;</td>
                <td>&nbsp;@*@Html.DisplayFor(model => model.RateofPermit)*@</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>7.</td>
                <td>Rate of permit fee (if any) per quart bottle</td>
                <td>&nbsp;</td>
                <td>Import fee @@ @Html.DisplayFor(model => model.RateofPermit)/- per Ltr.</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td>8.</td>
                <td>Amount of permit fee paid (if any)</td>
                <td>&nbsp;</td>
                <td>Rs.@Html.DisplayFor(model => model.TotalFees)/- T.C.No.@Html.DisplayFor(model => model.TransactionNo), Dated @Html.DisplayFor(model => model.EntryDate1)</td>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>SBI GOVT. BUSSINESS BRANCH LUCKNOW</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="4">Under Bond</td>
            </tr>
            <tr>
                <td colspan="4">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="4" style="text-align:right"><strong>DISTRICT EXCISE OFFICER</strong> </td>
            </tr>
            <tr>
                <td colspan="4" style="text-align:right"><strong>Lucknow</strong> </td>
            </tr>
        </table>
    </center>
</div>