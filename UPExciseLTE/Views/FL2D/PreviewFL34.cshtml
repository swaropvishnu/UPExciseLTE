@model UPExciseLTE.Models.FormFL33

@{
    ViewBag.Title = "PreviewFL34";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">
        function PrintPanel() {
            var panel = document.getElementById("divPrint");
            var printWindow = window.open('', '', 'height=400,width=800');
            printWindow.document.write('<html><head><title>FORM F.L.-34</title>');
            printWindow.document.write('<link href="../Content/PrintCss.css" rel="stylesheet" />');
            printWindow.document.write('</head><body >');
            printWindow.document.write(panel.innerHTML);
            printWindow.document.write('</body></html>');
            printWindow.document.close();
            setTimeout(function () {
                printWindow.print();
            }, 500);
            return false;
        }
</script>

<link href="~/Content/PrintCss.css" rel="stylesheet" />
<div style="text-align:right;width:100%;background-color:white;">
    <button id="btnPrint" onclick="PrintPanel();" class="btn green" style="margin-right:50px;margin-top:50px">Print FL 34</button>
</div>
<div style="width:100%;background-color:white" id="divPrint">
    <center>
        <table style="width:85%;background-color:white;text-orientation:upright">
            <tr>
                <td colspan="4" style="text-align:center;font-size:24.0pt;font-family:'Times New Roman';font-weight:bold;margin: 0cm;margin-bottom:.0001pt;"> FORM F.L.-34</td>
            </tr>
            <tr>
                <td colspan="4" style="text-align:center;font-size:18.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">Permit for the import Foreign Liquor</td>
            </tr>
            <tr>
                <td colspan="4" style="text-align:center;font-size:18.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">District Lucknow , Uttar Pradesh</td>
            </tr>
            <tr>
                <td colspan="4" style="font-size:10.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="4" style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">Permit for the import of Foreign Liquor No:- @Html.DisplayFor(model => model.FL33ID)</td>
            </tr>
            <tr>
                <td colspan="4" style="font-size:10.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
            </tr>
            <tr>
                <td colspan="2" valign="top" style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">Dated:- @Html.DisplayFor(model => model.FromPermitDate1)</td>
                <td colspan="2" valign="top" style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">Current to:- @Html.DisplayFor(model => model.ToPermitDate1)</td>
            </tr>
            <tr>
                <td colspan="4" style="font-size:10.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
            </tr>
            <tr>
                <td style="width:4%;font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;" valign="top">1.</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;" rowspan="2" valign="top">Name and Address of consigner</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;" valign="top">@Html.DisplayFor(model => model.FromConsignorName)</td>
            </tr>
            <tr>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;" valign="top">@Html.DisplayFor(model => model.FromConsignorAddress)</td>
