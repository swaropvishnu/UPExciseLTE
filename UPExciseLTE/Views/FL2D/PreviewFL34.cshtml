                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;" valign="top">Rs.@Html.DisplayFor(model => model.TotalFees)/- T.C.No.@Html.DisplayFor(model => model.TransactionNo), Dated @Html.DisplayFor(model => model.EntryDate1)</td>
            </tr>
            <tr>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;">&nbsp;</td>
                <td style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;" valign="top">SBI GOVT. BUSSINESS BRANCH LUCKNOW</td>
