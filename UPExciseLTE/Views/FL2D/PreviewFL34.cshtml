                    <table style="width:100%" class="tableCss">
                        <tr>
                            <th style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;"> <span style='margin-left:5px'> SNo</span> </th>
                            <th style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;"> <span style='margin-left:5px'> Brand Name</span></th>
                            <th style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;"> <span style='margin-left:5px'> No of Cases</span></th>
                            <th style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;"> <span style='margin-left:5px'> Total Bottles / Cans</span></th>
                            <th style="font-size:12.0pt;font-family:'Times New Roman';margin: 0cm;margin-bottom:.0001pt;"> <span style='margin-left:5px'> Total BL</span></th>
