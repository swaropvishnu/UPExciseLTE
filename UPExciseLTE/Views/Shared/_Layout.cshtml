    <script>
        $(document).ready(function () {
            myfunction();
        });
        function myfunction() {
            var d1 = @UserSession.LoggedInUserLevelId.ToString();
            if (d1 == "20") {
                $('.green').addClass('red');

            }
            else if (d1 == "25") {
                $('.green').addClass('yellow');
            }
            else if (d1 == "50") {
                $('.green').addClass('purple');

            }
            else if (d1 == "55") {
                $('.green').addClass('blue');
            }
        }
    </script>
    <style>
        .yellow {
            background-color: #f3ec64;
            /*color: red*/
